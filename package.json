{
	"name": "@farm-investimentos/front-mfe-components",
	"version": "3.5.3",
	"author": "farm investimentos",
	"private": false,
	"main": "./dist/front-mfe-components.common.js",
	"unpkg": "dist/front-mfe-components.umd.min.js",
	"jsdelivr": "dist/front-mfe-components.umd.min.js",
	"scripts": {
		"serve": "vue-cli-service serve",
		"build": "npm run build:components",
		"build:components": "vue-cli-service build --target lib --name front-mfe-components src/main.js",
		"lint": "vue-cli-service lint",
		"storybook": "start-storybook -p 6006",
		"build-storybook": "build-storybook",
		"test:unit": "jest --updateSnapshot",
		"test:coverage": "jest --updateSnapshot --coverage"
	},
	"files": [
		"dist/*",
		"src/*"
	],
	"dependencies": {
		"core-js": "3.21.1",
		"sass": "1.32.13",
		"sass-loader": "^10.1.1",
		"vue": "^2.6.11",
		"vuetify": "^2.5.7",
		"vuetify-dialog": "^2.0.16"
	},
	"devDependencies": {
		"@socheatsok78/storybook-addon-vuetify": "^0.1.8",
		"@storybook/addon-actions": "^6.3.6",
		"@storybook/addon-essentials": "^6.3.6",
		"@storybook/addon-links": "^6.3.6",
		"@storybook/vue": "^6.3.6",
		"@vue/cli-plugin-babel": "4.5.15",
		"@vue/cli-plugin-eslint": "~4.5.14",
		"@vue/cli-plugin-unit-jest": "~4.5.14",
		"@vue/cli-service": "~4.5.14",
		"@vue/test-utils": "^1.3.0",
		"babel-eslint": "^10.1.0",
		"eslint": "^6.7.2",
		"eslint-plugin-vue": "^6.2.2",
		"storybook-addon-designs": "^6.2.1",
		"v-mask": "^2.3.0",
		"vue-template-compiler": "^2.6.11",
		"webpack-cli": "^4.9.0"
	},
	"eslintConfig": {
		"root": true,
		"env": {
			"node": true
		},
		"extends": [
			"plugin:vue/essential",
			"eslint:recommended"
		],
		"parserOptions": {
			"parser": "babel-eslint"
		},
		"rules": {}
	},
	"browserslist": [
		"> 1%",
		"last 2 versions",
		"not dead"
	],
	"repository": {
		"type": "git",
		"url": "git://github.com/Farm-Investimentos/front-mfe-components.git"
	},
	"publishConfig": {
		"@farm-investimentos:registry": "https://registry.npmjs.org"
	}
}
